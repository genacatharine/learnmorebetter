<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head.ejs %>
  </head>
  <body>
    <div class="container" align="center">
      <h1><%= title %></h1>
      <div class="container col-xs-1" align="center">
        <div class="row" align="center">
          <img src="/images/galvanize_logo_orange_bg_200x200_360.jpg" class="circle">
          <br>
          <h4>Login Here</h4>
          <form>
            <div class="form-group" align="center">
              <label for="exampleInputEmail1">Email address</label>
              <input type="email" class="form-control" id="loginEmail" aria-describedby="emailHelp" placeholder="Enter email">
              <!-- <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small> -->
            </div>
            <div class="form-group">
              <label for="exampleInputPassword1">Password</label>
              <input type="password" class="form-control" id="loginPassword" placeholder="Password">
            </div>
            <a class="waves-effect waves-light btn" id="loginbtn">Log In</a>
            <a class="waves-effect waves-light btn" id="registerbtn">Register</a>
          </form>
        </div>
      </div>
    </div>
    <script src="../javascripts/login.js"></script>
    <% include ../partials/footer.ejs %>
  </body>
</html>
